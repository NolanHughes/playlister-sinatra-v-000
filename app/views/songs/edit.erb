<h1>Update a Song</h1>

<form action="/songs/<%= @song.slug %>" method="POST">
  <label>Name:</label>

  <br></br>

  <input type="text" id="Name" name="name" />

  <br></br>

  <label>Update the artist:</label>
  <label>Artist Name:</label>
  <br></br>
  <input type="text" name="artist_name" id="Artist Name"></input>
  <br></br>
  <input type="submit" value="Save">
</form>
